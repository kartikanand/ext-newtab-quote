{
  "manifest_version": 3,
  "name": "New Tab Quote",
  "version": "1.0.0",
  "description": "Display inspirational quotes on new tab pages",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
